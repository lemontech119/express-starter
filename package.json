    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "helmet": "^4.6.0",
    "mysql": "^2.18.1",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
